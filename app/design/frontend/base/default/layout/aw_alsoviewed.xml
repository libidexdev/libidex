<layout version="1.0.0">
    <default>
        <reference name="head">
            <action method="addItem">
                <type>skin_css</type>
                <name>aw_alsoviewed/css/styles.css</name>
            </action>
        </reference>
    </default>
    <catalog_product_view>
        <reference name="right">
            <block type="aw_alsoviewed/products" name="catalog.product.aw_alsoviewed.community" before="-"
                   template="catalog/product/list/alsoviewed.phtml">
                <action method="setTarget">
                    <alias>community</alias>
                </action>
            </block>
        </reference>
    </catalog_product_view>
</layout>